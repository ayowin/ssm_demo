		<dependency>
			<groupId>org.mybatis</groupId>
			<artifactId>mybatis-spring</artifactId>
			<version>2.0.0</version>
		</dependency>

		<!-- 使用mysql，导入mysql驱动包，兼容到5.1.1版本 -->
